{
  "name": "concorda",
  "version": "0.0.1",
  "description": "Seneca plugin for Concorda system",
  "author": "Mircea Alexandru (http://alexandrumircea.ro/)",
  "contributors": [
    "Mihai Dima",
    "Dean McDonnell (http://mcdonnelldean.me/)",
    "Cristian Kiss",
    "Karolina Szczur (http://thefox.is)"
  ],
  "main": "./lib/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/nearform/concorda.git"
  },
  "scripts": {
    "test": "lab test/*.test.js -r console -v -L -m 3000",
    "lint": "lab test -dL",
    "copy": "copyfiles -f ./client/assets/img/*.png ./dist/img",
    "build": "npm run copy & webpack --progress --colors",
    "watch": "webpack -w",
    "start": "./start.sh ./config/production.env ./server/start.js",
    "start:dev": "./start.sh ./config/development.env ./server/start.js"
  },
  "dependencies": {
    "async": "1.5.x",
    "bell": "6.3.0",
    "chairo": "2.0.0",
    "concorda-core": "github:nearform/concorda-core",
    "hapi": "13.0.0",
    "hapi-auth-cookie": "6.0.0",
    "jsonic": "0.2.2",
    "lodash": "3.10.1",
    "seneca-auth": "mirceaalexandru/seneca-auth",
    "seneca-github-auth": "github:mirceaalexandru/seneca-github-auth",
    "seneca-google-auth": "mirceaalexandru/seneca-google-auth",
    "seneca-mesh": "0.5.0",
    "seneca-twitter-auth": "github:mirceaalexandru/seneca-twitter-auth"
  },
  "devDependencies": {
    "code": "2.1.x",
    "eslint-config-seneca": "1.1.x",
    "eslint-plugin-hapi": "4.0.x",
    "eslint-plugin-react": "^3.16.1",
    "eslint-plugin-standard": "1.3.x",
    "lab": "8.0.x"
  }
}
